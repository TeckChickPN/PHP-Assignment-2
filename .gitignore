<?php
// Function to print the multiplication table
function printMultiplicationTable($number) {
    for ($i = 1; $i <= 10; $i++) {
        $result = $number * $i;
        echo $number . ' x ' . $i . ' = ' . $result . "\n";
    }
}

// Example usage
$givenNumber = 7;
echo "Multiplication table of " . $givenNumber . ":\n";
printMultiplicationTable($givenNumber);
?>


